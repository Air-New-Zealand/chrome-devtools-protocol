{
  "name": "jakubkulhan/chrome-devtools-protocol",
  "type": "library",
  "description": "Chrome Devtools Protocol client for PHP",
  "keywords": [
    "chrome",
    "chrome-devtools",
    "chrome-devtools-protocol",
    "headless-chrome",
    "web",
    "automation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Kulhan",
      "email": "jakub.kulhan@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "~7.1",
    "textalk/websocket": "^1.2"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "~0.9",
    "nette/php-generator": "~2.1|~3.0",
    "phpstan/phpstan": "~0.8",
    "phpunit/phpunit": "~6.4",
    "symfony/filesystem": ">=2.3"
  },
  "autoload": {
    "psr-4": {
      "ChromeDevtoolsProtocol\\": [
        "src/ChromeDevtoolsProtocol/",
        "gen-src/ChromeDevtoolsProtocol/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ChromeDevtoolsProtocol\\": [
        "test/ChromeDevtoolsProtocol/"
      ]
    }
  }
}
